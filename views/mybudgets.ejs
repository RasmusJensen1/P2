<main>
  <div class="top-container">
    <button class="butt butt-primary">Delete Selected</button>
    <button class="butt butt-primary">Upload (icon)</button>
    <div class="tool-tip">?</div>
  </div>
  <div class="budgets-container">
    <table>
      <thead>
        <tr>
          <th>Nr.</th>
          <th>Budget name</th>
          <th>Total</th>
          <th>Style</th>
          <th>Last edited</th>
          <th>Date created</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% let i = 1; %> <% budget_list.forEach((budget) => { %>
        <tr>
          <td>
            <input type="checkbox" />
            <%= i %>
          </td>
          <td><%= budget.name %></td>
          <td><%= budget.totalIncome %> dkk</td>
          <td>Basic</td>
          <td><%= new Date(budget.updatedAt).toLocaleDateString() %></td>
          <td><%= new Date(budget.createdAt).toLocaleDateString() %></td>
          <td>edit</td>
          <% i++; %> <% }); %>
        </tr>
      </tbody>
    </table>
  </div>
</main>
